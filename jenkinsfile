pipeline {
   agent any

   stages {
     stage(launch VM){
       steps {
         sh ansible-playbook playbook/ec2.yml
       }
     }

      stage(update host entry){
         steps {
           sh bash script/updatehost.sh
         }
      }

      stage(install and configure webserver){
           steps {
             sh ansible-playbook playbook/webserver.yml
           }
      }

   }
}
